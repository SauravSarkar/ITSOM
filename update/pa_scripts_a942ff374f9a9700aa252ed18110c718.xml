    <pa_scripts action="INSERT_OR_UPDATE">
        <arguments>estimated_hours,actual_hours</arguments>
        <description/>
        <name>Work.Capacity</name>
        <script><![CDATA[function getMilliSeconds(date){
	return (new GlideDateTime(date)).getNumericValue();
}
function calculateBrakeFactor(){
	return (getMilliSeconds(current.actual_hours)/getMilliSeconds(current.estimated_hours));
}
function calculateCapacity(){
	return 8/calculateBrakeFactor();
}
calculateCapacity();]]></script>
